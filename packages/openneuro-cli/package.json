{
  "name": "openneuro-cli",
  "version": "3.27.1",
  "description": "OpenNeuro command line uploader / editor.",
  "main": "index.js",
  "repository": "git@github.com:OpenNeuroOrg/openneuro.git",
  "engines": {
    "node": ">=12.9.0"
  },
  "os": [
    "!win32"
  ],
  "author": "Squishymedia",
  "license": "MIT",
  "bin": {
    "openneuro": "./src/index.js",
    "git-credential-openneuro": "./src/index.js",
    "git-annex-remote-openneuro": "./src/index.js"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "bids-validator": "1.5.9",
    "cli-progress": "^3.8.2",
    "commander": "^2.15.1",
    "cross-fetch": "^3.0.5",
    "elastic-apm-node": "^3.7.0",
    "esm": "^3.0.16",
    "fetch-h2": "^2.5.1",
    "find-config": "^1.0.0",
    "graphql-tag": "^2.10.3",
    "inquirer": "^5.2.0",
    "mkdirp": "^0.5.1",
    "openneuro-client": "^3.27.1"
  },
  "scripts": {
    "openneuro": "node src/index.js",
    "git-credential-openneuro": "node src/index.js",
    "git-annex-remote-openneuro": "node src/index.js"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/*.spec.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!bids-validator).+\\.js$"
    ]
  },
  "devDependencies": {
    "jest-fetch-mock": "^3.0.3",
    "metro-memory-fs": "0.61.0"
  }
}
