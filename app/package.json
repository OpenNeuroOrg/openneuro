{
  "name": "CRN",
  "version": "1.6.1",
  "description": "React JS web frontend for the CRN application.",
  "license": "MIT",
  "main": "app.js",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "start":
      "webpack-dev-server --config webpack.dev.js --progress --watch --color",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint --ext .jsx --ext .js src/scripts/",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "codecov": "codecov -p ../ -F app",
    "precommit": "lint-staged"
  },
  "author": "Squishymedia",
  "dependencies": {
    "async": "^2.6.0",
    "babel-polyfill": "^6.6.1",
    "bids-validator": "^0.25.0",
    "bowser": "^1.7.3",
    "bytes": "^3.0.0",
    "draft-js": "^0.10.5",
    "draft-js-prism": "^1.0.5",
    "favico.js": "git://github.com/ejci/favico.js#0.3.10",
    "load-google-api": "^0.1.3",
    "loadable-components": "^0.4.0",
    "moment": "^2.10.3",
    "pluralize": "^7.0.0",
    "prismjs": "^1.10.0",
    "prop-types": "^15.6.0",
    "raven-js": "^3.20.1",
    "react": "16",
    "react-bootstrap": "0.31.3",
    "react-dom": "16",
    "react-ga": "^2.2.0",
    "react-helmet": "^5.2.0",
    "react-router": "4",
    "react-select": "^1.0.0-rc.5",
    "react-table": "^6.7.6",
    "reflux": "6.4.1",
    "remarkable": "1.7.1",
    "spark-md5": "^3.0.0",
    "superagent": "^3.6.0",
    "url-search-params-polyfill": "^2.0.1",
    "urlite": "^1.2.8"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "codecov": "^3.0.0",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "3",
    "eslint": "4.16.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.6",
    "git-rev-sync": "^1.9.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.0.0",
    "lint-staged": "^6.0.1",
    "moment-timezone": "^0.5.13",
    "node-sass": "^4.5.3",
    "prettier": "^1.7.0",
    "react-addons-test-utils": "^15.6.2",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "watchify": "^3.9.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "jest": {
    "setupFiles": ["./jestsetup.js"],
    "snapshotSerializers": ["<rootDir>/node_modules/enzyme-to-json/serializer"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/*.spec.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,json}": ["prettier --write", "git add"]
    }
  }
}
