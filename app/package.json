{
  "name": "CRN",
  "version": "1.1.4",
  "description": "React JS web frontend for the CRN application.",
  "license": "MIT",
  "main": "app.js",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --progress --watch --color",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint --ext .jsx --ext .js src/scripts/",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "codecov": "codecov -p ../ -F app",
    "precommit": "lint-staged"
  },
  "author": "Squishymedia",
  "dependencies": {
    "async": "^1.2.1",
    "babel-polyfill": "^6.6.1",
    "bids-validator": "^0.23.11",
    "bowser": "^1.7.3",
    "bytes": "^2.3.0",
    "favico.js": "git://github.com/ejci/favico.js#0.3.10",
    "load-google-api": "^0.1.3",
    "moment": "^2.10.3",
    "opbeat-react": "^3.4.0",
    "pluralize": "^1.1.2",
    "prop-types": "^15.6.0",
    "react": "16",
    "react-bootstrap": "0.31.3",
    "react-dom": "16",
    "react-ga": "^2.2.0",
    "react-router": "4",
    "react-select": "^1.0.0-rc.5",
    "reactable": "vladnicula/reactable",
    "reflux": "6.4.1",
    "remarkable": "1.6.2",
    "spark-md5": "^1.0.0",
    "superagent": "^3.6.0",
    "urlite": "^1.2.8"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-eslint": "7",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-add-react-displayname": "^0.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "codecov": "^2.3.0",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "enzyme": "3",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "3",
    "eslint": "3.x",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lint-staged": "^4.2.1",
    "moment-timezone": "^0.5.13",
    "node-sass": "^4.5.3",
    "prettier": "^1.7.0",
    "react-addons-test-utils": "^15.6.2",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "watchify": "^2.4.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/*.spec.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "lint-staged": {
    "gitDir": "../",
    "linters": {
      "*.{js,jsx,json}": [
        "prettier --write",
        "git add"
      ]
    }
  }
}
